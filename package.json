{
  "name": "webpack-boilerplate",
  "version": "4.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS and lodash on board",
  "author": "Christoph von Gellhorn",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "npm": "^6.14.6"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.14.1",
    "sass-loader": "7.0.1",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "4.1.2"
  }
}
